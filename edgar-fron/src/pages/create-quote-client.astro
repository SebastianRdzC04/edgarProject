---
import LayoutPublic from "../layouts/LayoutPublic.astro";
import FormQuotesClient from "../components/react_components/FormQuotesClient.tsx";

const apiUrl = import.meta.env.PUBLIC_API_URL;
---

<LayoutPublic title="Client Quote">
    <div class="container mx-auto px-4 py-6">
        <FormQuotesClient apiUrl={apiUrl} client:load />
    </div>
</LayoutPublic>
